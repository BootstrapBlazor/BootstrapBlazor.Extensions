<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>8.3.0</Version>
  </PropertyGroup>

  <PropertyGroup>
    <PackageTags>Bootstrap Blazor WebAssembly wasm UI Components SqlSugar</PackageTags>
